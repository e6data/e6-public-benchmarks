        <HTTPSamplerProxy guiclass="HttpTestSampleGui" testclass="HTTPSamplerProxy" testname="${Selection}/api/GetProductSummaryDUSTrendOutput_DB">
          <stringProp name="HTTPSampler.domain">${__P(mainhost)}</stringProp>
          <stringProp name="HTTPSampler.port">443</stringProp>
          <stringProp name="HTTPSampler.protocol">https</stringProp>
