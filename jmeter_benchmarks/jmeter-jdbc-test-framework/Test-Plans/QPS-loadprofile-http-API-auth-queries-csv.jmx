          <elementProp name="HTTPsampler.Arguments" elementType="Arguments">
            <collectionProp name="Arguments.arguments">
              <elementProp name="" elementType="HTTPArgument">
                <boolProp name="HTTPArgument.always_encode">false</boolProp>
                <stringProp name="Argument.value">{&#xd;
  &quot;statement&quot;: &quot;with rel_week as ( select earliest_release_date, b.date_dbid, a.product_class_dbid from espresso_gold_test.uk_audio.mv_product_filter a join espresso_gold_test.\&quot;global\&quot;.dim_date b on CAST(a.earliest_release_date AS DATE) = CAST(b.\&quot;date\&quot; AS DATE) where product_dbid = ${product_dbid} and b.country_id = 1 and b.audio_video_flag = &apos;Audio&apos; ), t2 as ( select date_dbid, timeperiod_type, timeperiod_value, \&quot;year\&quot;, \&quot;week\&quot;, \&quot;quarter\&quot;, \&quot;month\&quot;, \&quot;period\&quot;, is_current from espresso_gold_test.uk_audio.dus_trend_timeperiod where timeperiod_type = &apos;Weekly&apos; ), t8 as ( select a.date_dbid, a.current_position as pos, a.product_dbid, b.\&quot;period\&quot;, b.timeperiod_type from espresso_gold_test.default.chart_entries_optimised a join espresso_gold_test.\&quot;global\&quot;.default_charts c on a.chart_dbid = c.chart_dbid and a.country_id = c.country_id join t2 b on a.date_dbid = b.date_dbid and c.audio_video_flag in (&apos;Audio&apos;, &apos;Both&apos;) where c.tab_name = &apos;Dus Treand&apos; and a.country_id = 1 and a.product_dbid = ${product_dbid} and a.audio_video_flag in (&apos;Audio&apos;, &apos;Both&apos;) and a.current_position &lt;= c.published_num and b.timeperiod_type = &apos;Weekly&apos; group by a.date_dbid, a.current_position, a.product_dbid, b.\&quot;period\&quot;, b.timeperiod_type ), trt as ( select distinct subcom_dbid, com_grp_dbid, com_grp_name from espresso_gold_test.\&quot;global\&quot;.mv_map_cla_com_grp_to_transaction_type where country_id = 1 and audio_video_flag in (&apos;Audio&apos;, &apos;Both&apos;) and product_class_dbid =( select product_class_dbid from rel_week ) ), main as( select date_dbid, \&quot;year\&quot;, \&quot;week\&quot;, timeperiod_type, timeperiod_value, is_current, com_grp_dbid, subcom_dbid, com_grp_name from trt cross join t2 ), t7 AS ( SELECT b.date_dbid, b.\&quot;year\&quot;, b.\&quot;week\&quot;, b.timeperiod_type, b.timeperiod_value, b.is_current, case when b.com_grp_dbid in (6, 7, 9, 15, 16, 18, 24, 25, 27) then ROUND(SUM(a.pre_ratio_sales)) else ROUND(SUM(a.weighted_sales)) end as DUS, case when b.com_grp_dbid in (6, 7, 9, 15, 16, 18, 24, 25, 27) then ROUND(SUM(a.pre_ratio_sales)) else ROUND(SUM(a.bva_weighted_sales)) end AS BVA, b.com_grp_dbid, b.com_grp_name FROM espresso_gold_test.uk_audio.agg_sub_com3 a right join main b ON a.date_dbid = b.date_dbid and a.product_dbid = ${product_dbid} and a.subcom_dbid = b.subcom_dbid GROUP BY b.date_dbid, b.\&quot;year\&quot;, b.\&quot;week\&quot;, b.timeperiod_type, b.timeperiod_value, b.is_current, b.com_grp_dbid, b.com_grp_name ), t_final as ( select a.date_dbid, a.\&quot;year\&quot;, a.\&quot;week\&quot;, a.timeperiod_type, a.timeperiod_value, a.is_current, a.DUS, a.BVA, a.com_grp_dbid, a.com_grp_name, case when DUS is null then null else b.pos end as Pos from t7 a left join t8 b on a.date_dbid = b.date_dbid and a.timeperiod_type = b.timeperiod_type ), t9_sub as ( select a.date_dbid, a.timeperiod_type, a.timeperiod_value, a.is_current, case when a.date_dbid &gt;= c.date_dbid then ifnull(a.DUS, 0) else null end as DUS, case when a.date_dbid &gt;= c.date_dbid then ifnull(a.BVA, 0) else null end as BVA, ((a.DUS - b.DUS) * 100 / b.DUS) as change_percentage, ((a.BVA - b.BVA) * 100 / b.BVA) as change_percentage_bva, a.com_grp_dbid, a.com_grp_name as commercial_model, a.Pos from t_final a left join t_final b on a.\&quot;year\&quot; -1 = b.\&quot;year\&quot; and a.com_grp_dbid = b.com_grp_dbid and a.timeperiod_type = b.timeperiod_type and a.\&quot;week\&quot; = b.\&quot;week\&quot; left join rel_week c on true ), t9 as ( select date_dbid, timeperiod_type, timeperiod_value, is_current, DUS, BVA, change_percentage, change_percentage_bva, com_grp_dbid, commercial_model, Pos, row_number() over( partition by timeperiod_type, is_current, com_grp_dbid order by date_dbid ) as display_order from t9_sub ) select 1 as country_id, &apos;Audio&apos; as audio_video_flag, b.*, null as var_percentage from t9 b order by is_current asc, date_dbid asc&quot;,&#xd;
  &quot;catalog&quot;: &quot;${CATALOG}&quot;,&#xd;
  &quot;schema&quot;: &quot;${SCHEMA}&quot;&#xd;
}</stringProp>
                <stringProp name="Argument.metadata">=</stringProp>
