ase and a.product_dbid_release=b.product_dbid_release ) , t4 AS ( SELECT distinct transaction_type_dbid,subcom_dbid,subcom_name FROM espresso_gold_test.\&quot;global\&quot;.mv_map_com_grp_to_transaction_type WHERE country_id = 1 AND audio_video_flag = &apos;Audio&apos; ) , t3_base as(select item_dbid,transaction_type_dbid,weighted_sales,bva_weighted_sales,retail_Value,bva_retail_value,pre_ratio_sales,product_class_dbid from mv_sales_inline_ctes a where item_dbid in (select distinct item_dbid_release from t1) ) ,t3 AS ( SELECT a.item_dbid as item_dbid_release, CASE WHEN c.subcom_dbid in (1,3,5) THEN ROUND(SUM(a.weighted_sales)) WHEN c.subcom_dbid &lt;&gt; 1 AND a.product_class_dbid IN (3, 6) THEN ROUND(SUM(a.weighted_sales)) ELSE ROUND(SUM(a.pre_ratio_sales)) END AS cummulative_DUS, CASE WHEN c.subcom_dbid = 1 THEN ROUND(SUM(a.bva_weighted_sales)) WHEN c.subcom_dbid &lt;&gt; 1 AND a.product_class_dbid IN (3, 6) THEN ROUND(SUM(a.bva_weighted_sales)) ELSE ROUND(SUM(a.pre_ratio_sales)) END AS cummulative_BVA, CASE WHEN c.subcom_dbid NOT IN (1, 3, 5) THEN ROUND(SUM(a.weighted_sales)) ELSE NULL END AS cummulative_DUS_calc, CASE WHEN c.subcom_dbid NOT IN (1, 3, 5) THEN ROUND(SUM(a.bva_weighted_sales)) ELSE NULL END AS cummulative_BVA_calc, c.subcom_dbid, c.subcom_name FROM t3_base a JOIN t4 c ON a.transaction_type_dbid = c.transaction_type_dbid GROUP BY a.item_dbid,c.subcom_dbid,c.subcom_name,a.product_class_dbid ) , main AS ( SELECT DISTINCT a.*, t3.subcom_dbid, t3.subcom_name FROM ( SELECT DISTINCT product_dbid_release, product_dbid, item_dbid_release, item_dbid FROM t1 ) a CROSS JOIN t3 ) ,main_data as ( select item_dbid,subcom_dbid,weighted_sales,pre_ratio_sales,retail_value,date_dbid from agg_item_inline_ctes where item_dbid in (select distinct item_dbid_release from main) and date_dbid=27209 ) ,t4_final as ( select b.product_dbid, b.item_dbid_release, a.date_dbid, b.subcom_dbid, a.weighted_sales, 2 as product_class_dbid, a.pre_ratio_sales, b.subcom_name, null as bva_weighted_sales, d.cummulative_DUS, d.cummulative_BVA, d.cummulative_DUS_calc, d.cummulative_BVA_calc from main_data a RIGHT JOIN main b ON b.item_dbid_release = a.item_dbid AND b.subcom_dbid = a.subcom_dbid LEFT JOIN t3 d ON b.item_dbid_release = d.item_dbid_release AND b.subcom_dbid = d.subcom_dbid ) , t5 AS ( SELECT a.product_dbid, a.item_dbid_release, a.date_dbid, CASE WHEN a.subcom_dbid in (1,3,5) THEN ROUND(SUM(a.weighted_sales)) WHEN a.subcom_dbid &lt;&gt; 1 AND a.product_class_dbid IN (3, 6) THEN ROUND(SUM(a.weighted_sales)) ELSE ROUND(SUM(a.pre_ratio_sales)) END AS DUS, CASE WHEN a.subcom_dbid NOT IN (1, 3, 5) THEN ROUND(SUM(a.weighted_sales)) ELSE NULL END AS DUS_calc, CASE WHEN a.subcom_dbid NOT IN (1, 3, 5) THEN ROUND(SUM(a.bva_weighted_sales)) ELSE NULL END AS BVA_calc, CASE WHEN a.subcom_dbid = 1 THEN ROUND(SUM(a.bva_weighted_sales)) WHEN a.subcom_dbid &lt;&gt; 1 AND a.product_class_dbid IN (3, 6) THEN ROUND(SUM(a.bva_weighted_sales)) ELSE ROUND(SUM(a.pre_ratio_sales)) END AS BVA, a.subcom_dbid AS com_grp_dbid, a.subcom_name AS commercial_model, SUM(a.cummulative_DUS) AS cummulative_DUS, SUM(a.cummulative_BVA) AS cummulative_BVA, SUM(a.cummulative_DUS_calc) AS cummulative_DUS_calc, SUM(a.cummulative_BVA_calc) AS cummulative_BVA_calc FROM t4_final a GROUP BY a.product_dbid, a.item_dbid_release, a.subcom_dbid, a.subcom_name, a.date_dbid, a.product_class_dbid ) , t6 AS ( SELECT DISTINCT b.product_dbid_release, b.product_dbid, b.product_class_dbid, b.item_dbid_release, a.date_dbid, b.format AS Format, b.\&quot;variant\&quot; AS \&quot;variant\&quot;, b.label AS Label, b.record_company_name AS Company, b.corp_name AS \&quot;Corp.Group\&quot;, b.distributor_name AS Distributor, b.catalogue_number AS \&quot;Cat.No.\&quot;, b.barcode_or_isrc AS Barcode, b.orphan_barcodes, b.primary_isrc, b.artist AS Artist, b.app_date AS \&quot;App. Date\&quot;, Deleted, a.com_grp_dbid, a.commercial_model, b.title AS Title, b.mix_name, CAST(b.side AS INT) AS Side, CAST(b.sequence AS INT) AS Sequence, b.volume AS Volume, b.ISRC, IFNULL(a.DUS, 0) AS DUS, IFNULL(a.DUS_calc, 0) AS DUS_calc, IFNULL(a.cummulative_DUS, 0) AS cummulative_DUS, IFNULL(a.cummulative_DUS_calc, 0) AS cummulative_DUS_calc, IFNULL(a.BVA, 0) AS BVA, IFNULL(a.BVA_calc, 0) AS BVA_calc, IFNULL(a.cummulative_BVA, 0) AS cummulative_BVA, IFNULL(a.cummulative_BVA_calc, 0) AS cummulative_BVA_calc FROM t5 a LEFT JOIN t1 b ON a.item_dbid_release = b.item_dbid_release and a.product_dbid = b.product_dbid ) , t9 AS ( SELECT product_dbid_release, product_dbid, product_class_dbid, item_dbid_release, date_dbid, Format, \&quot;variant\&quot;, Label, Company, \&quot;Corp.Group\&quot;, Distributor, \&quot;Cat.No.\&quot;, Barcode, orphan_barcodes, primary_isrc, Artist, \&quot;App. Date\&quot;, null as price_band, Deleted, com_grp_dbid, commercial_model, Title, mix_name, Side, Sequence, Volume, ISRC, DUS, cummulative_DUS, BVA, cummulative_BVA FROM t6 UNION SELECT product_dbid_release, product_dbid, product_class_dbid, item_dbid_release, date_dbid, Format, \&quot;variant\&quot;, Label, Company, \&quot;Corp.Group\&quot;, Distributor, \&quot;Cat.No.\&quot;, Barcode, orphan_barcodes, primary_isrc, Artist, \&quot;App. Date\&quot;, null as price_band, Deleted, com_grp_dbid, CONCAT(commercial_model, &apos;_calc&apos;) AS commercial_model, Title, mix_name, Side, Sequence, Volume, ISRC, DUS_calc, cummulative_DUS_calc, BVA_calc, cummulative_BVA_calc FROM t6 WHERE com_grp_dbid NOT IN (1, 3, 5) ) SELECT * FROM t9&quot;,&#xd;
